on init
    chown system system /sys/bus/i2c/drivers/es9218-codec/avc_volume
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/avc_volume
    chown system system /sys/bus/i2c/drivers/es9218-codec/headset_type
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/headset_type
    chown system system /sys/bus/i2c/drivers/es9218-codec/left_volume
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/left_volume
    chown system system /sys/bus/i2c/drivers/es9218-codec/right_volume
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/right_volume
    chown system system /sys/bus/i2c/drivers/es9218-codec/ess_filter
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/ess_filter
    chown system system /sys/bus/i2c/drivers/es9218-codec/ess_custom_filter
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/ess_custom_filter
    chown system system /sys/bus/i2c/drivers/es9218-codec/override_impedance
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/override_impedance
    chown system system /sys/bus/i2c/drivers/es9218-codec/override_to_custom_filter
    chmod 0660 /sys/bus/i2c/drivers/es9218-codec/override_to_custom_filter

on property:sys.boot_completed=1
    start vendor.lge-dac-control-2-0

service vendor.lge-dac-control-2-0 /vendor/bin/hw/vendor.lge.hardware.audio.dac.control@2.0-service.proprietary
    class hal
    user system
    group system
    disabled
